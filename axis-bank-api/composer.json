{
  "name": "opsway/axis-bank-api",
  "description": "PHP SDK for Axis bank",
  "license": "MIT",
  "keywords": [
    "Axis",
    "API",
    "SDK",
    "library"
  ],
  "support": {
    "issues": "https://github.com/opsway/axis-bank-api/issues"
  },
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.3.3",
    "ext-json": "*",
    "ext-gnupg": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0.2",
    "squizlabs/php_codesniffer": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "OpsWay\\Axis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpsWay\\Axis\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --no-coverage",
    "test-cover": "phpunit",
    "cs-check": "phpcs -n --standard=PSR2 --extensions=php src/",
    "cs-fix": "phpcbf --standard=PSR2 --extensions=php src/"
  }
}